{
  "name": "dependency-impact-visualizer",
  "displayName": "Dependency Impact Visualizer",
  "description": "Real-time visual graph showing how changes to functions/classes ripple through your codebase",
  "version": "0.0.3",
  "publisher": "CODEWEB",
  "icon": "media/Gemini_Generated_Image_2s5pog2s5pog2s5p.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "license": "MIT",
  "author": {
    "name": "Sarthak Singh",
    "email": "sarthaksingh1211@gmail.com",
    "url": "https://sarthaksingh-96.github.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SarthakSingh-96/codeweb"
  },
  "bugs": {
    "url": "https://github.com/SarthakSingh-96/codeweb/issues"
  },
  "homepage": "https://github.com/SarthakSingh-96/codeweb#readme",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dependency-impact-visualizer.showImpact",
        "title": "Show Dependency Impact"
      },
      {
        "command": "dependency-impact-visualizer.analyzeSymbol",
        "title": "Analyze Symbol Impact"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "dependency-impact-visualizer.analyzeSymbol",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "esbuild": "^0.27.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
